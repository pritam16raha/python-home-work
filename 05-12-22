def factorial(number):
    if number == 1:
        return 1
    else:
        return(number * factorial(number - 1))
number = int(input())
print("Factorial of", number, "is: ", factorial(number))

def mul_fun(n,i):
    if(i>10):
        return
    print(n, "*", i, "=", n*i)
    return mul_fun(n, i+1)

print("Enter the Number: ")
num = int(input())
print(mul_fun(num,1))
#mul_fun(num,1)


def gcd(a, b):
   if a == b:
      return a
   elif a < b:
      return gcd(b, a)
   else:
      return gcd(b, a - b)

a = int(input("type a \n"))
b = int(input("type b \n"))
print(gcd(a, b))


